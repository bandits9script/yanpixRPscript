        freezeButton.BackgroundColor3 = Color3.fromRGB(60, 0, 0)
        if freezeConnection then
            freezeConnection:Disconnect()
            freezeConnection = nil
        end
        unfreezeAllPlayers()
    end
end)

buttonInstances["SPEED: OFF"].MouseButton1Click:Connect(function()
    if isSpeedActive then
        if speedConnection then speedConnection:Disconnect() end
        isSpeedActive = false
        buttonInstances["SPEED: OFF"].Text = "SPEED: OFF"
        buttonInstances["SPEED: OFF"].BackgroundColor3 = Color3.fromRGB(60, 0, 0)
        local character = player.Character
        if character then
            local humanoid = character:FindFirstChild("Humanoid")
            if humanoid then humanoid.WalkSpeed = 16 end
        end
    else
        startSpeedGlitch()
        buttonInstances["SPEED: OFF"].Text = "SPEED: ON"
        buttonInstances["SPEED: OFF"].BackgroundColor3 = Color3.fromRGB(0, 80, 0)
    end
end)


local buttonInstances = {}

local function addButtonToPage(page, name, x, y, w, h, textSize)
    local btn = Instance.new("TextButton")
    btn.Name = name:gsub(" ", "_")
    btn.Size = UDim2.new(0, w or 160, 0, h or 32)
    btn.Position = UDim2.new(0, x or 0, 0, y or 0)
    btn.BackgroundColor3 = Color3.fromRGB(60, 0, 0)
    btn.Text = name
    btn.TextColor3 = Color3.fromRGB(255, 255, 255)
    btn.Font = Enum.Font.SourceSansBold
    btn.TextSize = textSize or 14
    btn.BorderSizePixel = 0
    btn.ZIndex = 10001
    btn.Parent = page
    
    local btnTextStroke = Instance.new("UIStroke")
    btnTextStroke.Color = Color3.fromRGB(0, 0, 0)
    btnTextStroke.Thickness = 2
    btnTextStroke.Parent = btn
    
    local uc = Instance.new("UICorner")
    uc.CornerRadius = UDim.new(0,8)
    uc.Parent = btn
    local stroke = Instance.new("UIStroke")
    stroke.Color = Color3.fromRGB(255, 0, 0)
    stroke.Thickness = 2
    stroke.Parent = btn
    
    local glowEffect = Instance.new("UIStroke")
    glowEffect.Color = Color3.fromRGB(255, 100, 100)
    glowEffect.Thickness = 4
    glowEffect.Transparency = 1
    glowEffect.Parent = btn
    glowEffect.Name = "GlowEffect"
    
    btn.MouseEnter:Connect(function()
        TweenService:Create(btn, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(100, 0, 0)}):Play()
        TweenService:Create(stroke, TweenInfo.new(0.2), {Color = Color3.fromRGB(255, 100, 100)}):Play()
        TweenService:Create(glowEffect, TweenInfo.new(0.2), {Transparency = 0}):Play()
    end)
    
    btn.MouseLeave:Connect(function()
        if not string.find(btn.Text, "ON") then
            TweenService:Create(btn, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(60, 0, 0)}):Play()
        else
            TweenService:Create(btn, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(0, 80, 0)}):Play()
        end
        TweenService:Create(stroke, TweenInfo.new(0.2), {Color = Color3.fromRGB(255, 0, 0)}):Play()
        TweenService:Create(glowEffect, TweenInfo.new(0.2), {Transparency = 1}):Play()
    end)
    
    buttonInstances[name] = btn
    return btn
end

addButtonToPage(pages["Main"], "HITBOX: OFF", 10, 8, 180, 32, 14)
addButtonToPage(pages["Main"], "AUTO FIRE: OFF", 10, 52, 180, 32, 14)
addButtonToPage(pages["Main"], "KILL ALL: OFF", 10, 96, 180, 32, 14)


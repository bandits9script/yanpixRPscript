    if isSpinActive then
        if spinConnection then spinConnection:Disconnect() end
        isSpinActive = false
        buttonInstances["SPIN: OFF"].Text = "SPIN: OFF"
        buttonInstances["SPIN: OFF"].BackgroundColor3 = Color3.fromRGB(60, 0, 0)
    else
